<serviceGroup>
        <service name="UserManagerAxisService" scope="application">
        <description>
            new spring example
        </description> 
        
           <parameter name="ServiceObjectSupplier">org.apache.axis2.extensions.spring.receivers.SpringAppContextAwareObjectSupplier
        </parameter>
        
        <parameter name="SpringBeanName" locked="false">UserManagerAxisService</parameter>
        <parameter name="SpringContextLocation">config/spring/spring-common.xml</parameter>
        <messageReceivers> 
            <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-only"
                class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver" />
            <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out"
                class="org.apache.axis2.rpc.receivers.RPCMessageReceiver" />
        </messageReceivers>
   </service>
